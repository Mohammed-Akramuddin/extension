{
  "manifest_version": 3,
  "name": "Face Age SafeSearch Toggle",
  "version": "1.0.0",
  "description": "Analyzes a face with an ONNX model to determine minor/major and toggles SafeSearch accordingly.",
  "icons": {
    "16": "favicon-16x16.png",
    "32": "favicon-32x32.png",
    "48": "favicon-48x48.png",
    "128": "favicon-128x128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "SafeSearch via Face Analysis",
    "default_icon": {
      "16": "favicon-16x16.png",
      "19": "favicon-16x16.png",
      "32": "favicon-32x32.png",
      "38": "favicon-48x48.png",
      "48": "favicon-48x48.png",
      "128": "favicon-128x128.png"
    }
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": [
    "storage",
    "declarativeNetRequest",
    "windows"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["gatekeeper.js"],
      "run_at": "document_start"
    }
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "declarative_net_request": {
    "rule_resources": []
  },
  "web_accessible_resources": [
    {
      "resources": [
        "swin_face_classifier.onnx",
        "lib/*",
        "consent.html",
        "consent.css",
        "consent.js"
      ],
      "matches": [
        "*://*/*",
        "chrome-extension://*/*"
      ]
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
  }
}
